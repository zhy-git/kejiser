@CHARSET "UTF-8";
*{-webkit-tap-highlight-color: rgba(0,0,0,0);}
body{overflow-y: hidden;}
body.noSelect{-moz-user-select: none;-o-user-select:none;-khtml-user-select:none;-webkit-user-select:none;-ms-user-select:none;user-select:none;}



.tab_speak li:before,
.btn_dd .btn_recording:before,
.others_box li a:before,
.others_box li i:before,
.right_info .btn_like:before,
.right_info .btn_hate:before,
.cbox_main .dd_1 a:before,
.btn_closeCBox,
.btn_gtb_close,
.ql_timer span:before,
.live_redbag .redbag_cancel,.otherredmoney_content .gene_cancel,
.write_dan:before,.comment_input_box .commentInput:before,.comment_input_box .shutupbox:before,
.guest_head b,.pageMenu .page_refresh,
.allcount i,.collecttips i,.collecttips:after,.backtips_i1,.backtips_i2,.backtips:after,
.btn_gotoCreateLive:before,
.tab_recordingType:before,
.img_view_window .btn_closed,
dt.luckyMoney .lm_main:before,
[class*="_bubble"] .btn_revoke:before,
[class*="_bubble"] .btn_resend{background:url(../resource/live_logo.png?v=2016050601) no-repeat; background-size:50rem;}





.speakBox,.commentBox{position: absolute; left: 0; top: 0; width: 100%; height:100%; overflow: hidden;}


/***发言区***/
/*头部*/
.liveHeader{position: absolute; z-index: 3; width: 100%; padding: 1rem; top: 0; left: 0;}
.guest_head{ position: relative; float: left;padding: 0.2rem;}
.guest_head i{position: relative; z-index: 2; float: left; width: 2.5rem; height: 2.5rem; margin: 0 -0.5rem 0 0; overflow: hidden; border-radius: 3rem; border: solid 1px #fff;}
.guest_head i img{ width: 100%; height: 100%; border-radius: 3rem;}
.guest_head b{ float: left; width: 2rem; height: 2rem; background-position:-2rem -2rem ;position: relative;z-index: 2;margin: 0.2rem 0 0 0.2rem; }
.guest_head:before{content: "";position: absolute; left: 0; top: 0; width: 100%; height: 100%; background: #000; opacity: 0.5;border-radius: 5rem;}


/*侧边菜单*/
.pageMenu{position:absolute; z-index: 4; margin: 0 0 1rem; right: 1rem; bottom: 1rem;}
.pageMenu li{ float: right; width: 4rem; height: 4rem; margin:0 0 1.5rem; text-align: center; line-height: 3.8rem; color: #333333; font-size: 1.4rem; background: #fff; border: solid 1px #e5e5e5; border-radius: 3rem;cursor: pointer;}
.pageMenu li a{ display: block; width: 100%; height: 100%;}
.pageMenu .page_refresh{background-size:52rem;background-position: -27.7rem -3.9rem; margin:0 0 3rem;background-color:#fff;}
.pageMenu li.tabToComment{ color: #fff; background: #00acff; border: none;}
.pageMenu li.tabToIndex{ font-size: 1.2rem;line-height: 1.4rem;}
.pageMenu li.tabToIndex a{padding: 0.5rem 0px 0px;}


/*直播消息区*/
.speakContentBox{  position: absolute; width: 100%;  top: 0; bottom: 0; padding: 4rem 0 0;overflow: auto;overflow-x: hidden; background: #f2f2f2;-webkit-overflow-scrolling : touch;}
.bubbles_dl{ display: block;  padding: 1rem 0;}

.bubble_dt{ display:block; margin:1rem auto; height:2rem; line-height: 2rem; text-align: center;}
.bubble_dt b{ position: relative; display: inline-block; padding: 0 1rem; font-weight: normal;z-index: 1; color: #fff;}
.bubble_dt b:before{ content: ""; position: absolute; z-index: -1; left:0; top: 0; height: 100%;  width: 100%; background: #ccc; opacity: 0.5; border-radius: 3px;}

[class*="_bubble"]{position: relative; display: block; padding: 1rem 1rem 0 5.5rem; *zoom:1;min-height: 12rem;}
[class*="_bubble"]:after {visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}

[class*="_bubble"] .head_portrait{ position: absolute; left: 1rem; top: 5.5rem; width: 3rem; height: 3rem; border-radius: 3rem; overflow: hidden;}
.head_portrait img{ width: 100%; height: 100%;border-radius: 3rem;}
[class*="_bubble"] .speak_time{ display: block; height: 2rem; padding: 0 4.5rem 0 0; line-height: 2rem; text-align: center; color: #b3b3b3; font-size: 1.2rem;}
[class*="_bubble"] .speaker_name{ font-size: 1.2rem; line-height: 2.5rem; color: #999;}
[class*="_bubble"] .speaker_name b{ color: #555;padding: 0 0.5rem 0 0; font-size: 1.4rem; font-weight: normal;max-width: 20rem; overflow: hidden;display: inline-block;vertical-align: top; text-overflow: ellipsis; white-space: nowrap;}
[class*="_bubble"] .bubble_content{ position: relative; float: left; max-width: 70%;min-width: 5rem; min-height: 3.6rem; padding: 0.5rem; margin: 0 4rem 0 0; background: #fff; border: solid 1px #e5e5e5; border-radius: 3px; line-height: 2.4rem; cursor: pointer;}

[class*="_bubble"] .bubble_content:before{content: ""; position: absolute;top:1.2rem; width: 0px; height: 0px;border-top: 0.5rem solid transparent;border-bottom: 0.5rem solid transparent;}
[class*="_bubble"] .bubble_content:after{content: ""; position: absolute;top:1.2rem; width: 0px; height: 0px;border-top: 0.5rem solid transparent;border-bottom: 0.5rem solid transparent;}
.left_bubble .bubble_content:before { left: -0.8rem;border-right: 0.8rem solid #e5e5e5;}
.left_bubble .bubble_content:after { left: -0.6rem;border-right: 0.8rem solid #fff;}
.right_bubble .bubble_content:before { right: -0.8rem;border-left: 0.8rem solid #e5e5e5;}
.right_bubble .bubble_content:after { right: -0.6rem;border-left: 0.8rem solid #fff;}

.bubble_content p{padding: 0 0.5rem; color: #999; font-size: 1.6rem; word-break: break-all;}
.bubble_content p b{ color: #333; padding: 0 0.4rem 0 0; font-weight: normal;}
.bubble_content p:last-child{ color: #333;}
.bubble_content img{ max-width: 100%; display: block;}
.bubble_content p:nth-child(2){border-top: dashed 1px #bcbcbc;}

[class*="_bubble"] .btn_ilike{ position: absolute; top:8rem; left:1.2rem; width:2.5rem;height:2.5rem; line-height:2.3rem; text-align: center; border-radius:2.5rem;background-color:#edcb91;color:#b98d42;font-size:1.6rem;margin-top: 1rem; font-weight: bold;}
[class*="_bubble"] .btn_revoke{position: relative; float: left; width: 3rem; height: 3rem; margin: -1.2rem 0 0 -5.9rem; padding: 0.5rem 0 0; line-height: 3rem; text-align: center; color: #d1d1d1; cursor: pointer;}
[class*="_bubble"] .btn_revoke:before{content: ""; width: 2rem; height: 2rem; display: inline-block; background-position: -38rem -2rem; background-color: rgba(0,0,0,0.3); border-radius: 2rem;}
[class*="_bubble"] .btn_resend{ display:none; float: left; width: 3rem; height: 3rem; margin: 0.2rem 0 0; background-position: -39rem -4rem; cursor: pointer;}

.bubble_content.recordingMsg,
.bubble_content.recordTemp{ position: relative; width: 16rem; height: 4rem; cursor: pointer;}
.recordingMsg i,
.recordTemp i{display: inline-block; width: 2rem; height:2rem; margin: 0.5rem 1rem 0 0.5rem; background:url(../img/live/audioPlaying.png) 0rem 0rem no-repeat;background-size:2rem;  -webkit-user-select: none; -moz-user-select: none; -ms-user-select: none; vertical-align: bottom;}
.recordingMsg.isPlaying i{background:url(../resource/audioPlaying.gif) 0rem 0rem no-repeat;background-size:2rem; }
.recordingMsg.audioloading i{background-image:url(../resource/logo_loading_3.gif);}
.recordingMsg var,
.recordTemp var{ position: absolute; right: -4rem; top: 1.5rem;  width: 3rem; height: 2.5rem; color: #666;}
.recordingMsg var:after,
.recordTemp var:after{content: '"'; padding: 0 0 0 0.2rem;}
.recordingMsg var:before{content: ""; position: absolute; left: 0; top: -1rem; width: 0.8rem; height: 0.8rem; border-radius: 1rem; background: #f94747; }
.chexiaoaudio{background-color:#ccc;color:#fff; padding: 0.5rem 1rem; border-radius: 3rem;}
.audio_make_ul{text-align:left;margin-top:1rem;font-size:1.5rem;line-height:2.5rem;}


.right_bubble{ padding: 1rem 5.5rem 0 1rem; }
.right_bubble .head_portrait{left: auto; right: 1rem;}
.right_bubble .speak_time{padding: 0 0 0 4.5rem;}
.right_bubble .speaker_name{ text-align: right;}
.right_bubble .bubble_content{ float: right; margin: 0 0 0 4rem;}
.right_bubble .btn_ilike{left: auto; right:1.2rem;}
.right_bubble .btn_revoke{ float: right; margin:0.3rem -4rem 0 0;}
.right_bubble .recordingMsg{margin: 0 0 0 7rem;}
.right_bubble .recordingMsg var{ left: -4rem; right: auto;  text-align: right;}
.right_bubble .recordingMsg var:before{left: auto; right: 0;}

.recordingMsg.isReaded var:before{ display: none;}

.recordingMsg.recordwid1,.recordTemp.recordwid1{width:10rem;}
.recordingMsg.recordwid2,.recordTemp.recordwid2{width:12rem;}
.recordingMsg.recordwid3,.recordTemp.recordwid3{width:14rem;}
.recordingMsg.recordwid4,.recordTemp.recordwid4{width:16rem;}
.recordingMsg.recordwid5,.recordTemp.recordwid5{width:18rem;}




.luckyMoney .bubble_content{width:20rem; height: 6rem; background: #ffaf32; padding: 1.1rem 0.5rem 0.5rem 5rem; border: 0;}
.luckyMoney .bubble_content:before{position:absolute; left: 0.6rem; top: 0.5rem; border: none; width:4rem;height:5rem;background:url(../img/live/ico_luckyMoney.png) center center no-repeat;background-size: 3.4rem 4.4rem;vertical-align:top;}
.luckyMoney .bubble_content:after {border-left: 0.8rem solid #ffaf32;}
.luckyMoney .bubble_content p{ display: block; line-height: 1.8rem;  color: #fff; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; word-break: break-all;}

.bubbles_dl dt.luckyMoney{ display:block; margin:1rem auto; text-align: center;}
.bubbles_dl dt.luckyMoney .lm_main{ position: relative; display: inline-block; height:2.5rem; line-height: 2.4rem; font-size: 1.4rem; padding: 0 0.5rem 0 0.2rem; font-weight: normal;z-index: 1; color: #fff; background-color:#edcb91; border-radius: 4px; cursor: pointer;  white-space: nowrap; overflow: hidden; text-overflow: ellipsis; max-width: 80%;}
dt.luckyMoney .lm_main b{ color: #d7493d; font-weight: normal;}
dt.luckyMoney .lm_main:before{ content: ""; display: inline-block; width: 2rem; height: 2rem; vertical-align: top; margin: 0.2rem 0 0; background-position:-24rem 0 ;}
.bubbles_dl dt.luckyMoney .lm_main var{ max-width: 10rem; overflow: hidden; text-overflow: ellipsis; display: inline-block; vertical-align: top;}

/*直播底部*/
.recing_bg{display:none; position: absolute; z-index: 4; width: 100%; height: 100%; cursor: pointer;}
.speakBottom{ display: none; position:absolute; z-index: 4;  width: 100%; bottom: 0; height: 0; overflow: hidden; box-shadow: 0 0 4px #ccc;}

.speakBottom .tab_speak{ display: none; width: 100%; height: 5rem; line-height: 5rem; background: #fff; border-bottom: solid 1px #e8e8e8;}
.tab_speak li{ display: inline-block; width: 33.3%; text-align: center; font-size: 1.4rem; color: #888888;border-right: solid 1px #e8e8e8; cursor: pointer;}
.tab_speak li:last-child{border: none;}
.tab_speak li:before{ content: ""; width: 2rem; height: 2rem; display: inline-block;vertical-align: top;margin:1.4rem 0.2rem 0 0;}
.tab_speak .tab_voice:before{background-position: -12rem 0;}
.tab_speak .tab_text:before{background-position: -16rem 0;}
.tab_speak .tab_others:before{background-position: -20rem 0;}

.recording_tab_box{ display: none; position: relative; width: 100%; height: 21rem; padding: 4rem 0 0; background: #fff; overflow: hidden;white-space: nowrap;}
.recording_box,.recording_click{ display: inline-block; width: 100%; height: 100%; transition: 0.5s; -webkit-transition: 0.5s; margin-left: 0; vertical-align: top;}
.recording_tab_box .second_dd{ display:none; position: absolute; left: 0; top: 0; width: 100%; height: 3rem; margin: 1rem 0 0; line-height: 3rem; text-align: center; color: #00acff; font-size: 1.4rem;}

.recording_tab_box .second_dd i:first-child{background:url(../img/live/ico_voice_1.png) right center no-repeat;}
.recording_tab_box .second_dd i:last-child{  background:url(../img/live/ico_voice_2.png) left center no-repeat;}
.recording_tab_box .second_dd i{display: inline-block; width: 5rem; height: 1rem; margin: 1rem 1rem 0; vertical-align: top;  background-size:auto 100%;}

.recording_tab_box .rec_Title_box{position: absolute; left: 0; top: 2rem; display: block; width: 100%; height: 3rem; line-height: 3rem; text-align: center; font-size: 1.4rem; color: #888;}
.recording_tab_box .rec_Title_box b{ font-weight: normal;}
.recording_tab_box .rec_Title_box b:last-child{display: none;}


.recording_tab_box .btn_dd{ position: relative; display: block; width: 100%; padding:2rem 0 ;  text-align: center;}
.recording_tab_box .btn_recording{position: relative; display: inline-block; width: 9rem; height: 9rem; border-radius: 9rem; line-height: 9rem; font-size: 1.4rem; color: #fff; text-align: center; background-color: #00acff;  background-position: 0 -14rem; cursor: pointer;}
.btn_dd .btn_recording:before{content: ""; display: inline-block; width: 6rem; height: 6rem; margin: 1.5rem 0 0; background-position: 0 -14rem;}
.btn_recording.stop_rec{ display: none;}
.btn_recording.send_rec{ display: none;}
.btn_recording.start_rec{ border: solid 1px #d4d4d4; background: #fff;}
.btn_recording.start_rec:before{ width:6.8rem; height: 6.8rem; margin: 1rem 0 0; border-radius: 7rem; background:#f15959;}
.btn_recording.stop_rec:before{background-position:-12rem -14rem;}
.btn_recording.send_rec:before{ display: none;}
.recording_tab_box .tips_dd{ display: block; height: 2rem; line-height: 2rem; font-size: 1.4rem; color: #888888; text-align: center;}
.btn_dd.startRec .btn_recording.start_rec,.btn_dd.stopRec .btn_recording.start_rec{ display: none;}
.btn_dd.startRec .btn_recording.stop_rec,.btn_dd.stopRec .btn_recording.send_rec{display: inline-block;}

.btn_dd .btn_cancel_clickRec{position: absolute; z-index: 4; top:-2.5rem; right: 1rem;  width: 5rem; height: 2.5rem; line-height: 2.5rem; cursor: pointer; text-align: center; color: #888888; border: solid 1px #888888; border-radius: 3rem;}
.btn_dd.stopRec .btn_cancel_clickRec{ display: block;}

.send_rec object{position: absolute; left: 0; top: 0;}

.others_box{ display:none; padding: 2rem 0 0;}
.others_box li{ display: inline-block; width:32%;text-align: center;}
.others_box li a,.others_box li i{ display: inline-block; width: 6rem; line-height: 2rem;}
.others_box li a:before,.others_box li i:before{ content: ""; display: block; width: 5rem; height: 5rem; margin: 0 0.5rem; border: solid 1px #ccc; border-radius: 3px;}
.others_box .btn_img i:before{background-position: 0.4rem -9.5rem;}
.others_box .btn_link a:before{background-position: -3.5rem -9.5rem;}
.others_box .btn_video a:before{background-position: -7.6rem -9.5rem;}

.text_input_box{ display:none;height: 5rem;padding: 0.5rem 1rem 0;}
.text_input_box .boxFlex{ position:relative; padding: 0 0.5rem;}
.text_input_box .btnBackVoice{ display: inline-block; width: 5rem; height: 4rem; text-align: center; line-height: 4rem; font-size: 1.4rem; border: solid 1px #e5e5e5; border-radius: 3px; background: #fff;}
.text_input_box .speakInput{ display:block; width: 100%; height: 4rem; line-height: 2rem;padding:0.9rem 0.5rem; font-size: 1.6rem; border: 1px solid #e5e5e5;background-color:#f8f8f8;  border-radius: 3px; box-shadow: none; color: #333; resize: none;}
.text_input_box .btnLiveTalk{ display: inline-block; width: 5rem; height: 4rem; text-align: center; line-height: 4rem; font-size: 1.4rem;  border-radius: 3px; background: #00acff; color: #fff;}
.qlAndroid .ql_input_front{ position: absolute; top: 0; left: 0; width: 100%; height: 100%;}

.voiceBottom .speakBottom{ display: block; height: 26rem;}
.voiceBottom .speakContentBox,.voiceBottom .pageMenu{bottom: 26rem;}
.voiceBottom .tab_speak,.voiceBottom .recording_tab_box{ display: block;}
.voiceBottom .tab_voice:before{background-position: -14rem 0;}

.othersBottom .speakBottom{ display: block; height: 16rem;}
.othersBottom .speakContentBox,.othersBottom .pageMenu{bottom: 16rem;}
.othersBottom .tab_speak,.othersBottom .others_box{ display: block;}
.othersBottom .tab_others:before{background-position: -22rem 0;}

.textBottom .speakBottom{ display: block; height: 5rem;}
.textBottom .speakContentBox,.textBottom .pageMenu{bottom: 5rem;}
.textBottom .text_input_box{display:-webkit-box; display:box; display:flex; display:-webkit-flex; display:-ms-flexbox;}
.textBottom .tab_text:before{background-position: -18rem 0;}
.voiceBottom .tab_voice,.othersBottom .tab_others,.textBottom .tab_text{ color: #00acff;}

.hasTabBottom .speakBottom{display: block; height: 5rem;}
.hasTabBottom .speakContentBox,.hasTabBottom .pageMenu{bottom: 5rem;}
.hasTabBottom .tab_speak{ display: block;}


.recordingTips{display: none; position: absolute; width: 100%; height: 5rem; line-height: 5rem; text-align: center; font-size: 1.5rem; color: #fff; left: 0; top: 0;}
.rTips_1,.rTips_3,.rTips_5{background: rgba(0,162,255,0.9);}
.rTips_2,.rTips_4{background: rgba(255,95,99,0.9);}

.recording_tab_box .btn_dd i{ content: ""; position: absolute;left: 50%; top: 50%; width: 9rem;height: 9rem;  margin: -4.5rem 0 0 -4.5rem;border-radius: 9rem;background-color: #e5f4fc;}
.recording .recording_box .btn_dd i,.btn_dd.startRec i{  animation: recordingAnime 1s linear infinite alternate; -webkit-animation: recordingAnime 1s linear infinite alternate;  -moz-animation: recordingAnime 1s linear infinite alternate;}


.tab_recordingType{position: absolute; left: 50%; bottom: 0; width: 25.5rem;  height: 5rem; margin-left: -12.9rem; padding:0.5rem 0 0 8.5rem;  transition: 0.5s; -webkit-transition: 0.5s;}
.tab_recordingType:before{ content: ""; position: absolute; left: 50%; top: 0.5rem;width: 0.6rem;height: 0.6rem;margin: 0 0 0 -0.3rem; background: #00acff; border-radius: 1rem; }
.tab_recordingType a{position: relative; display: inline-block; width:8.5rem; height: 3rem; line-height: 3rem; text-align: center; color: #888; font-size: 1.4rem;}
.recType_2:after{ content: ""; position: absolute; left: 0; top: 1rem; width: 1px; height: 1rem; background: #e5e5e5;}
a.recType_1{color: #00acff;}

.changeRecType .recording_click{ margin-left: -100%;}
.changeRecType .rec_Title_box b:first-child{display: none;}
.changeRecType .rec_Title_box b:last-child{display: block;}
.changeRecType .tab_recordingType{padding-left: 0;}
.changeRecType a.recType_1{ color: #888;}
.changeRecType a.recType_2{ color: #00acff;}


/*..changeRecType .recType_2{display: block;}
.changeRecType .recType_1{display: none;}*/
.recording .second_dd{ display: block;}
.recording .tab_recordingType,
.recording .rec_Title_box{display: none;}


@keyframes recordingAnime
{
from {width: 9rem;height: 9rem;  margin: -4.5rem 0 0 -4.5rem;}
to {width: 15rem;height: 15rem;  margin: -7.5rem 0 0 -7.5rem;}
}
@-webkit-keyframes recordingAnime /* Safari 和 Chrome */
{
from {width: 9rem;height: 9rem;  margin: -4.5rem 0 0 -4.5rem;}
to {width: 15rem;height: 15rem;  margin: -7.5rem 0 0 -7.5rem;}
}
@-moz-keyframes recordingAnime 
{
from {width: 9rem;height: 9rem;  margin: -4.5rem 0 0 -4.5rem;}
to {width: 15rem;height: 15rem;  margin: -7.5rem 0 0 -7.5rem;}
}

.recordingSending{display:none; position: absolute;z-index: 4; bottom: 6.3rem; left: 50%; height: 3rem; width: 13rem; margin: 0 0 0 -6.5rem; line-height: 3rem; font-size: 1.3rem; text-align: center;}
.recordingSending:before{content: ""; position: absolute; display: block; left: 0; width: 100%; height: 100%; background: #000; opacity: 0.5; border-radius: 3px;}
.recordingSending b{position: relative; z-index: 4; color: #fff;}

/*登录*/
.login_box{position: absolute; display: block; bottom: 0; left: 0;  width: 100%; z-index: 2; overflow: hidden; box-shadow: 0px 0px 5px 1px #CCC; background: #fff;height: 5rem;}
.login_btn{width:90%;height:4rem;background-color:#00A8FF;color:#fff;display:block; line-height:4rem;font-size:2rem;text-align: center;margin:0.5rem auto;border-radius: 0.5rem;}





/***评论区***/
.commentBox{z-index: 4; display: none;}
.commentBox:before{content: ""; position: absolute;z-index: 3; left: 0; top: 0; width: 100%; height: 100%; background: #000; opacity: 0.5;}

.commentHeader{position: absolute;z-index: 4; top: 0; left: 0; padding: 1.2rem 0 0; width: 100%;  text-align: center;}
.backToLive{ display: inline-block; width: 9.5rem; height: 3.5rem; line-height: 3.5rem; background: #00acff; color: #fff; font-size: 1.5rem; border-radius: 2px;}

.commentContentBox{  position: absolute;z-index: 4; width: 100%;  top: 6rem; bottom: 5rem; padding: 0rem 0 0;overflow: auto;overflow-x: hidden; background: #fff;-webkit-overflow-scrolling : touch;}
.comment_dl{ display: block; padding:0;}
.comment_dl .comment_dd{display: block; padding: 1rem; background: #fff; *zoom:1; border-bottom: solid 1px #ebebeb;}
.comment_dl .comment_dd:after {visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.comment_dd .right_info{ float: right;height: 2rem;overflow: hidden;}
.right_info .btn_wall,
.right_info .btn_iswalled{ display: inline-block; height: 2rem;line-height: 2rem; border-radius: 2px; padding: 0 0.8rem; color: #00acff; background: #fff; border: solid 1px #dadada; font-size: 1.2rem; cursor: pointer;}
.right_info .btn_iswalled{ color: #aaa;}
.right_info .btn_like,
.right_info .btn_hate{ display: none; height: 2rem; margin: 0 0 0 1rem; line-height: 2rem; font-size: 1.2rem; color: #888888; cursor: pointer;}
.right_info .btn_like:before,
.right_info .btn_hate:before{ content: ""; display: inline-block; width: 2rem; height: 2rem; vertical-align: top; }
.right_info .btn_like:before{background-position: -4rem 0 ;}
.right_info .btn_hate:before{background-position: -6rem 0 ;}
.liked .right_info .btn_like:before{background-position: -8rem 0 ;}
.hated .right_info .btn_hate:before{background-position: -10rem 0 ;}


.comment_dd .avatar{ float: left; width: 4rem; height: 4rem; margin: 0 1rem 0 0; border-radius: 4rem; overflow: hidden;}
.comment_dd .avatar img{ width: 100%; height: 100%;}
.comment_dd .author_name{ display: block; height: 2rem; line-height: 2rem; color: #00acff; font-size: 1.2rem; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
